<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ProgLogicUIController.TestQuestionAppearController">
   <children>
      <Label fx:id="lblTotalQuestion" layoutX="125.0" layoutY="67.0" prefHeight="28.0" prefWidth="87.0" text="totalQuestion" textAlignment="CENTER" textFill="#039ed3" />
      <Label fx:id="lblTimeShow" layoutX="125.0" layoutY="95.0" prefHeight="28.0" prefWidth="87.0" text="Total Time" textAlignment="CENTER" textFill="#039ed3" />
      <StackPane fx:id="mcqStackPane" alignment="TOP_LEFT" layoutX="2.0" layoutY="145.0" prefHeight="201.0" prefWidth="567.0">
         <children>
            <Label fx:id="lblQuestion" alignment="TOP_LEFT" prefHeight="39.0" prefWidth="426.0" text="Q: How many generations of computers we have?" textFill="#039ed3" wrapText="true">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <StackPane.margin>
                  <Insets left="30.0" top="10.0" />
               </StackPane.margin>
               <font>
                  <Font name="Arial Bold" size="12.0" />
               </font>
            </Label>
            <CheckBox fx:id="checkOptionOne" mnemonicParsing="false" onAction="#handleCheckOptionOne" prefHeight="17.0" prefWidth="525.0" wrapText="true">
               <StackPane.margin>
                  <Insets left="30.0" top="70.0" />
               </StackPane.margin>
            </CheckBox>
            <CheckBox fx:id="checkOptionTwo" mnemonicParsing="false" onAction="#handleCheckOptionTwo" prefHeight="17.0" prefWidth="529.0" wrapText="true">
               <StackPane.margin>
                  <Insets left="30.0" top="100.0" />
               </StackPane.margin>
            </CheckBox>
            <CheckBox fx:id="checkOptionThree" mnemonicParsing="false" onAction="#handleCheckOptionThree" prefHeight="17.0" prefWidth="523.0" wrapText="true">
               <StackPane.margin>
                  <Insets left="30.0" top="130.0" />
               </StackPane.margin>
            </CheckBox>
            <CheckBox fx:id="checkOptionFour" mnemonicParsing="false" onAction="#handleCheckOptionFour" prefHeight="17.0" prefWidth="519.0" wrapText="true">
               <StackPane.margin>
                  <Insets left="30.0" top="160.0" />
               </StackPane.margin>
            </CheckBox>
         </children>
      </StackPane>
      <Button fx:id="btnSubmit" layoutX="252.0" layoutY="353.0" mnemonicParsing="false" onAction="#handleSubmitButton" prefHeight="25.0" prefWidth="87.0" stylesheets="@../design/style.css" text="Submit" />
      <Label fx:id="lblTestName" alignment="CENTER" layoutX="53.0" layoutY="14.0" prefHeight="28.0" prefWidth="484.0" text="Quiz" textAlignment="CENTER" textFill="#07526b" underline="true" wrapText="true">
         <font>
            <Font name="Broadway" size="31.0" />
         </font></Label>
      <Label layoutX="36.0" layoutY="67.0" prefHeight="28.0" prefWidth="87.0" text="Total Question:" textAlignment="CENTER" textFill="#039ed3">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="36.0" layoutY="95.0" prefHeight="28.0" prefWidth="87.0" text="Total Time       :" textAlignment="CENTER" textFill="#039ed3">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
